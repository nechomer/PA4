    C[] arr;

    //check fields, should print initial values
    Library.printi((new C()).f);
    Library.printb((new C()).b);
    Library.println((new C()).s);
    Library.printi((new C()).c.f);

    //check arr is null, not initialized, should print error
    arr[1].foo();
  }
}

